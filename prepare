sudo pacman -Syyu
mkdir clone
cd clone
git clone https://github.com/rashadalsharpini/linux-stuff
git clone https://github.com/rashadalsharpini/nvim
git clone https://github.com/rashadalsharpini/obsidian
cd
sudo pacman -S fish
chsh -s /usr/bin/fish
sudo pacman -S neovim
sudo pacman -S kitty
sudo cp -r ~/clone/nvim ~/.config/
sudo cp -r ~/clone/fish ~/.config/
sudo cp -r ~/clone/linux-stuff/kitty ~/.config/
sudo pacman -S btop
sudo pacman -S lsd
sudo pacman -S lf
sudo pacman -S mpv
sudo pacman -S obsidian

